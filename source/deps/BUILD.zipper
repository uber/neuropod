package(
    default_visibility = ["//visibility:public"],
)

cc_library(
    name = "zipper",
    srcs = glob([
        "zipper/**/*.c",
        "zipper/**/*.cpp",
    ], exclude = glob(["zipper/tps/**"])),
    hdrs = glob([
        "zipper/**/*.h",
    ]),
    includes = [
        "zipper"
    ],
    # Don't warn in third party libs
    copts = ["-Wno-everything"],
    deps = [
        "@minizip_repo//:minizip",
    ],
    visibility = ["//visibility:public"],
)
